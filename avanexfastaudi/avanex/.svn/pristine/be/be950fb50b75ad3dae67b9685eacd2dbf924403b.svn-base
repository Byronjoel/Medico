                <button id="btnDelete" label="Eliminar "  disabled="true"  width="80%" visible="false"   zclass="button" image="/images/delete.gif" />
                <space/>
                <button id="btnPreview" label="Preview  "   disabled="true"  width="80%"  visible="false" zclass="button" image="/images/preview.gif" />
                <space/>
                <button id="btnOpen" label="Abrir    "   disabled="true"  width="80%"  visible="false" zclass="button" image="/images/open.gif" />
                <space/>
                <button id="btnPrint" label="Imprimir "  disabled="true"  width="80%" visible="false"    zclass="button" image="/images/imprimir.gif" />
                <space/>
                <button id="btnSync" label="Syncronizar"  disabled="true" width="80%" visible="false"  zclass="button" image="/images/syncronizar.gif" />
