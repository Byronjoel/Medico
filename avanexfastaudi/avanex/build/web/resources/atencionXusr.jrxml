	<subDataset name="diagnosticos" uuid="6fdea917-318b-4537-a87b-387f0f22cb1c">
		<queryString>
			<![CDATA[SELECT
     ava_lisord."nro_ord" AS ORDEN,
     ava_lisord."fec_ord" AS FECHA,
     ava_petxord."cod_ana" AS ANALISIS,
     ava_lispac."nom_pac" AS NOMBRES,
      ava_lispac."ape_pac" AS APELLIDOS,
    ava_lisord."id_plan" AS PLAN
FROM
     "DBA"."ava_lisord" ava_lisord INNER JOIN "DBA"."ava_petxord" ava_petxord ON ava_lisord."nro_ord" = ava_petxord."nro_ord"
     AND ava_lisord."cod_suc" = ava_petxord."cod_suc"
     INNER JOIN "DBA"."ava_lispac" ava_lispac ON ava_lisord."cod_pac" = ava_lispac."cod_pac"
WHERE
     cod_ana = 'S579']]>
		</queryString>
